.index_card {
  width: 100%;
}

.card_box {
width: 90%;
display: flex;
padding: 5% 5%;
overflow-x:hidden;
border-bottom: 2rpx solid #e2e2e2;
}

.card_img_box {
  width: 180rpx;
  height: 180rpx;
  overflow: hidden;
  border-radius: 20rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
}

.card_img {
  width: 100%;
}

.card_info {
  width: calc(100% - 200rpx);
  padding-left: 20rpx;
}

.card_info1 {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.card_info1_text1 {
  color: #000;
  font-weight: 500;
  font-size: 36rpx;
}

.card_info1_text2 {
  color: rgb(141, 140, 140);
  font-size: 20rpx;
}

.card_info2_text1 {
  color: rgb(177, 177, 177);
  font-size: 26rpx;
}

.card_info3 {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.card_info3_text1 {
  color: #f60;
}

.card_info3_text1_1 {
  font-size: 20rpx;
}

.card_info3_text1_2 {
  font-size: 26rpx;
}

.card_info3_text1_3 {
  color: rgb(141, 140, 140);
  font-size: 30rpx;
}

.card_info3_text1_3_1 {
  font-size: 26rpx;
}

.card_info3_text2 {
  color: rgb(141, 140, 140);
  font-size: 26rpx;
}
