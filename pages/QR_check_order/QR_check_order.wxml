<view class='check_order-box'>
  <view class='check_order-content'>
   <input class='check_order-text'placeholder="请输入消费券码"name="phone" type="number" maxlength="11"  bindinput="bindKeyInput" value="{{inputValue}}" bindfocus="activeFocus" />
    <text class='check_btn-text' bindtap='check_phone_order'>{{currentText}}</text>
  </view>
</view>
<view  class='order_card_info'>
        <view  class='card_info1'>
          <text class='card_info1_text1'>订单编号</text>
          <text class='card_info1_text2'>{{item.orderNo}}</text>
        </view>
        <view  class='card_info1'>
          <text class='card_info1_text1'>订单时间</text>
          <text class='card_info1_text2'>{{item.title}}</text>
        </view>

        <view  class='card_info1'>
          <text class='card_info1_text1'>订单总价</text>
          <text class='card_info1_text2'>{{item.createTime}}</text>
        </view>
        <view  class='card_info1'>
          <text class='card_info1_text1'>订单状态</text>
          <text class='card_info1_text2'>{{item.payTime}}</text>
        </view>
        <view class="textarea text_26rpx">
          <textarea placeholder="请输入订单备注" placeholder-style="color:#000;"  />
        </view>
</view>
<view class='pro_box'>
    <view class='pro_box_content'>
        <view class='pro_box_img_box'>
          <image class='pro_box_img_box_img' src="../../static/images/user.png">
          </image>
        </view>
        <view  class='pro_box_info'>
          <view class='pro_box_info1'>
            <view class='pro_box_info1_name'>项目名称</view>
            <view class='pro_box_info1_desc'>项目描述</view>
            <view class='pro_box_info1_price'>项目价格</view>
          </view>
        </view>
    </view>
</view>
<view class='bottom_box background_F60'>
    <text class='text_36rpx color_Ffff font_weiget_blod'>核销 </text>
</view>
      