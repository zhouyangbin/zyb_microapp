.check_order-box {
  width: 700rpx;
  padding: 20rpx 30rpx;
  border-bottom: 2rpx solid #e2e2e2;
}
.check_order-content {
  background: white;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
}
.check_btn-text{
  min-width: 100rpx;
  min-height: 80rpx;
  text-align: center;
}
.check_order-text {
  flex: 8;
  margin-left: 20rpx;
  background: #f5f4f4;
  border: 2rpx solid #f0efef;
  border-radius: 30px;
  font-size: 26rpx;
  padding-left: 20rpx;
  height: 60rpx;
  line-height: 60rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.order_empty {
  width:100%;
  height: 250px;
  padding-top: 80rpx;
  padding-bottom: 80rpx;
  background-color: #fff;
  text-align: center;
}
.bgorder_empty_bg_img{
  width: 80px;
  height: 60px;
}
.container {
  width: 100%;
}
